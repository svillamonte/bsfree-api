{
  "disabled": false,
  "bindings": [
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 */6 * * *"
    },
    {
      "name": "shouts",
      "type": "queue",
      "queueName": "latest-shouts",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}